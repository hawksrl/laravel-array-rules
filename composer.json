{
    "name": "hawksrl/laravel-array-rules",
    "type": "library",
    "description": "Allows to write rules as an array",
    "license": "MIT",
    "authors": [
        {
            "name": "Enzo Notario",
           "email": "enzo.notario@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Hawk\\ArrayRules\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
